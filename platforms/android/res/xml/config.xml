<?xml version='1.0' encoding='utf-8'?>
<widget id="com.flyco.fsas" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SmartLinkConnect">
        <param name="android-package" value="com.phonegap.plugins.smartLinkConnect.SmartLinkConnect" />
    </feature>
    <feature name="EspressifConnect">
        <param name="android-package" value="com.phonegap.plugins.espressifConnect.EspressifConnect" />
    </feature>
    <feature name="cordovaWifiInfo">
        <param name="android-package" value="org.apache.cordova.wifiinfo.cordovaWifiInfo" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="IsFirst">
        <param name="android-package" value="com.phonegap.plugins.isFirst.IsFirst" />
    </feature>
    <feature name="FileOperate">
        <param name="android-package" value="com.phonegap.plugins.fileOperate.FileOperate" />
    </feature>
    <feature name="ThirdLogin">
        <param name="android-package" value="com.phonegap.plugins.thirdLogin.ThirdLogin" />
    </feature>
    <feature name="Baidushare">
        <param name="android-package" value="com.phonegap.plugins.baidushare.Baidushare" />
    </feature>
    <feature name="PicOperate">
        <param name="android-package" value="com.phonegap.plugins.picOperate.PicOperate" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="CodeScan">
        <param name="android-package" value="com.phonegap.plugins.codeScan.CodeScan" />
    </feature>
    <allow-intent href="market:*" />
    <name>fsas</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="main.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
