.swiper-pagination {
    position: absolute;
    z-index: 20;
    bottom: 10px;
    text-align: center;
    width: 100%;
}

.swiper-pagination span {
    display: inline-block;
    width: 9px;
    height: 9px;
    background: #dadada;
    margin: 0 2px;
    opacity: 0.8;
    cursor: pointer;
    border-radius: 5px;
}

.swiper-pagination span.on {
    background: #666666;
    border-radius: 5px;
}

.swiper-pagination .swiper-pagination-bullet {
    display: inline-block;
    width: 9px;
    height: 9px;
    background: #dadada;
    margin: 0 2px;
    opacity: 0.8;
    cursor: pointer;
    border-radius: 5px;
}

.swiper-pagination .swiper-pagination-switch {
    display: inline-block;
    width: 9px;
    height: 9px;
    background: #dadada;
    margin: 0 2px;
    opacity: 0.8;
    cursor: pointer;
    border-radius: 5px;
}

.swiper-pagination .swiper-active-switch {
    background: #666666;
    border-radius: 5px;
}

