<p style="display: none" align="center">
<textarea id="tpl_aboutDevice">
    <ul class="list">
        <li class="bigli">
            <div align="center" id="devicePic">
                <a href="#" id="deviceDtlImg">
                    {#if $T.image==""}
                    <img id="dtlP" src="./images/myDeviceDtl/{$T.productCode}demo.png"/>
                    {#else}
                    <img id="dtlP" src="{$T.image}"/>
                    {#/if}
                </a>
            </div>
        </li>
        <li class="divider">
        </li>
        <li>
            <div class="grid">
                <div class="col1-3">
                    <h3>设备昵称</h3>
                </div>
                <div class="col2-3">
                    <a id="toEditDeviceName" href="#"><p id="deviceNike">{$T.deviceName}</p></a>
                </div>
            </div>
        </li>
        <li>
            <div class="grid" >
                <div class="col1-3">
                    <h3>二维码</h3>
                </div>
                <div id="barCode" class="col2-3">
                    <!--<img style="with:35px; height: 35px" id="barCode" src="./images/myDeviceDtl/{$T.barCode}"/>-->
                </div>
            </div>
        </li>
        <li>
            <div class="grid">
                <div class="col1-3">
                    <h3>版本信息</h3>
                </div>
                <div class="col2-3">
                    <p id="deviceVersion">{$T.deviceVersion}</p>
                    <a href="#" id="deviceVersionFlag"></a>
                </div>
            </div>
        </li>
        <li>
            <div class="grid">
                <div class="col1-3">
                    <h3>产品信息</h3>
                </div>
                <div class="col2-3">
                    <p>{$T.information}</p>
                </div>
            </div>
        </li>
        <li>
            <div class="grid">
                <div class="col1-3">
                    <h3>规格参数</h3>
                </div>
                <div class="col2-3">
                    {#foreach $T.specifications as Item}
                    <p>{$T.Item.productSpecificationParam}:{$T.Item.productSpecificationValue}</p>
                    {#/for}
                </div>
            </div>
        </li>
        <li>
            <div class="grid">
                <div class="col1-3">
                    <h3>功能参数</h3>
                </div>
                <div class="col2-3">
                    {#foreach $T.functional as Item}
                    <p>{$T.Item.productFunctionParam}:{$T.Item.productFunctionValue}</p>
                    {#/for}
                </div>
            </div>
        </li>
        <li class="detail">
            <div class="grid">
                <div class="col1-3">
                    <h3>功能介绍</h3>
                </div>
                <div class="col2-3">
                    {#foreach $T.functionIntroduction as Item}
                    <p>{$T.Item.functionName}:{$T.Item.functionComments}</p>
                    {#/for}
                </div>
            </div>
        </li>
        <li class="divider"></li>
    </ul>
</textarea>
</p>