/*清除链接样式*/
a {
    text-decoration: none;
    color: #808080;
}

/*无效、灰显样式*/
.disabled {
    background: #ccc;
    border-color: #ccc;
    color:#fff;
}

/*1/3+2/3布局*/

.grid{
    color: #808080;
    font-size: 14px;
}
.col1-3 {
    text-align: center;
    margin-bottom: 0;
    font-size: 14px;

}

.col2-3 {
    margin-bottom: 0;
    font-size: 14px;
}

.col2-3:focus {
    border: none;
}

/*用户消息、设备故障中已读消息，未读消息颜色*/
.message {
    color: #000;
}

.unreadMessage {
    color: #969696;
}

/*弹出手动添加、扫一扫、搜索菜单*/
.popupDevice {
    /*border:1px solid #cccccc;*/
    /*border-radius: 20px;*/
    width: 10rem;
    position: fixed;
    top: 39px;
    right: 0;
    z-index: 100;
    display: none;
}

/*设备控制页中，弹出设备权限、设备粉丝、设备日志、设备消息、设备设置、关于设备*/
.popupMyDevice {
    /*border:1px solid #cccccc;*/
    /*border-radius: 20px;*/
    width: 10rem;
    position: fixed;
    top: 39px;
    right: 0;
    z-index: 100;
    display: none;
}

/*设备离线状态*/
.offline {
    -webkit-filter: grayscale(100%);
    -moz-filter: grayscale(100%);
    -ms-filter: grayscale(100%);
    -o-filter: grayscale(100%);
}

.popupMyFollowedDevice {
    /*border:1px solid #cccccc;*/
    /*border-radius: 20px;*/
    width: 10rem;
    position: fixed;
    top: 39px;
    right: 0;
    z-index: 100;
    display: none;
}

