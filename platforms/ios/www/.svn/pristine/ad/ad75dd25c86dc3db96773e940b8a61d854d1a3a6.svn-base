/*welcome*/
#welcome .content {
    position: relative;
    width: 100%;
    height: 100%;
}

#welcome .swiper-container {
    width: 100%;
    height: 100%;
}

#welcome .swiper-slide {
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 18px;
    background: #fff;
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}

#welcome .enter {
    margin: auto auto 30% auto;
    color: #fff;
    border: solid 1px #d4d4d4;
    padding: 4px;
    border-radius: 5px;
}

#welcome .swiper-slide:nth-child(1) {
    background: url(../images/welcome/welcome1.png);
    background-size: 100%;
}

#welcome .swiper-slide:nth-child(2) {
    background: url(../images/welcome/welcome2.png);
    background-size: 100%;
}

#welcome .swiper-slide:nth-child(3) {
    background: url(../images/welcome/welcome3.png);
    background-size: 100%;
}

