        .dialog-buttons { text-align:center; padding:7px 0; }
          .dialog-buttons .button { display: inline-block; width:152px; height:42px; line-height:42px; cursor: pointer; font-size:14px; color:#fff; background-color:#0c82d9; font-family:"Microsoft Yahei", sans-serif;}
          .dialog-buttons .button{ *display:inline;}
          .dialog-buttons .cancel{ background-color:#f3f3f3; color:#000;}
