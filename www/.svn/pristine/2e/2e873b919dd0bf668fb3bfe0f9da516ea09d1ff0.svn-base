<div class="ui-content login">
    <div class="ui-loginBox">
        <div class="inset" align="center">
            <input type="text" name="account" id="userName" required="required" maxlength="11"
                   pattern="^1[3|4|5|8][0-9]\d{4,8}$" title="请输入正确的手机号" placeholder="请输入手机号码">
            <input type="password" name="password" id="userPassWord" required="required" maxlength="16"
                   pattern="^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{8,16}$" title="密码必须为8-16位字母和数字的组合" placeholder="输入密码">
        </div>

        <div align="right">
            <a href="#forgetPassword">忘记密码？</a>
        </div>
        <div align="center">
            <div><a href="" class="button" id="doLogin">登录</a></div>
            <div><a href="#regist" class="button white cl doRegister" style="color:#2364ae;">注册</a></div>
        </div>
        </br>
