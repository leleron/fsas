<p style="display: none">
    <textarea id="refundDetail_itemAdd">
        <ul>
            {#if $T==""}
            <p style="text-align: center;">订单中不包含商品</p>
            {#else}
                {#foreach $T as Item}
                <li class="tkListItem">
                    <a goods_sn="{$T.Item.sn}" class="refundDetailItem">
                        <img src="{$T.Item.goods_thumb}" />
                        <strong>{$T.Item.name}</strong>
                        <!-- TODO 没有单价 -->
                        <span>
                            商城价：{$T.Item.shop_price}元&nbsp;&nbsp;&nbsp;&nbsp;数量：{$T.Item.goods_number}&nbsp;&nbsp;&nbsp;&nbsp;小计： 没有单价 元
                        </span>
                    </a>
                </li>
                {#/for}
            {/#if}
            </ul>
    </textarea>
</p>

