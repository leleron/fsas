<div class="applyTH applyRegNo">
    <form id="buyWithout" check_status="false" >
    <ul>
        <div id="div_promoMsg"></div>
        <!--<li class="tips">注册登陆用户获赠1000积分并可享受积分抵现</li>-->
        <li>
            <span>收货人姓名：</span>
            <input id="buyWithoutUserName" name="address_consignee" type="text" placeholder="请填写真实姓名"/>
        </li>
        <li>
            配送区域：
            <!--<div id="div_buyWithoutAddress_province"></div>
            <div id="div_buyWithoutAddress_city"></div>
            <div id="div_buyWithoutAddress_area"></div>-->
            <select id="buyWithoutAddress_province" name="address_province" class="acProvince"><option value="-1"> --请选择--  </option></select>
            <select id="buyWithoutAddress_city" name="address_city" class="acCity"><option value="-1"> --请选择--  </option></select>
            <select id="buyWithoutAddress_district" name="address_district" class="acDistrict"><option value="-1"> --请选择--  </option></select>
        </li>

        <li>
            <span>详细地址：</span>
            <input id="buyWithoutAddress_address" name="address_address" type="text" placeholder="不少于5个字"/>
        </li>

        <li>
            <span>邮政编码：</span>
            <input id="buyWithoutZipCode" name="address_zipcode"  type="text" maxlength="6" placeholder="6位数字"/>
        </li>

        <li>
            <span>固定电话：</span>
            <input id="buyWithoutPoneNumber" name="address_tel" type="text" maxlength="8" placeholder="8位数字"/>
        </li>

        <li>
            <span>手机号码：</span>
            <input id="buyWithoutMPoneNumber" name="address_mobile" type="text" maxlength="11" placeholder="11位数字"/>
        </li>

        <li>
            <span>验证码：</span>
            <input id="buyWithoutVericode" name="buyWithoutVericode" placeholder="填写验证码" type="text" />
            <div id="buyWithoutSendCode" class="getCode">获取动态验证码</div>
        </li>

    </ul>
        <a id="buyWithoutSubmit" class="blueBtn">提交</a>
    </form>
</div>


<!--</tpl>-->
<p style="display: none">
    <textarea id="tpl_promoMsg">
        <li class="tips">{$T}</li>
    </textarea>
    <textarea id="tpl_buyWithoutRegist">
        <option value="-1"> --请选择--  </option>
        {#foreach $T as Item}
            <option value="{$T.Item.region_id}">{$T.Item.region_name}</option>
        {#/for}
    </textarea>
</p>

